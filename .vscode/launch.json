    "version": "0.2.0",
    "configurations": [
        {
            "name": "PokerEnv",
            "type": "debugpy",
            "request": "launch",
            "program": "poker_env.py",
            "console": "integratedTerminal"
        },
        {
            "name": "DeepCRFPokerEnv",
            "type": "debugpy",
            "request": "launch",
            "program": "pokerenv_crf.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/deepcrf"
        },
        {
            "name": "DeepCRFTraining",
            "type": "debugpy",
            "request": "launch",
            "program": "train_deepcrf.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/deepcrf"
        },
        {
            "name": "TrainV0",
            "type": "debugpy",
            "request": "launch",
            "program": "train_v0.py",
            "console": "integratedTerminal",
            "env": {
                "CUDA_VISIBLE_DEVICES": "1"
            }
        },
        {
            "name": "Visualize",
            "type": "debugpy",
            "request": "launch",
            "program": "visualize.py",
            "console": "integratedTerminal"
        }
    ]
}