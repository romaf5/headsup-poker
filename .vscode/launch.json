    "version": "0.2.0",
    "configurations": [
        {
            "name": "PokerEnv",
            "type": "debugpy",
            "request": "launch",
            "program": "poker_env.py",
            "console": "integratedTerminal"
        },
        {
            "name": "DeepCFRPokerEnv",
            "type": "debugpy",
            "request": "launch",
            "program": "pokerenv_cfr.py",
            "cwd": "${workspaceFolder}/deepcfr",
            "console": "integratedTerminal"
        },
        {
            "name": "DeepCFRTraining",
            "type": "debugpy",
            "request": "launch",
            "program": "train_deepcfr.py",
            "cwd": "${workspaceFolder}/deepcfr",
            "console": "integratedTerminal",
            "env": {
                "CUDA_VISIBLE_DEVICES": "1"
            }
        },
        {
            "name": "DeepCFRPolicyEval",
            "type": "debugpy",
            "request": "launch",
            "program": "eval_policy.py",
            "cwd": "${workspaceFolder}/deepcfr",
            "console": "integratedTerminal",
            "env": {
                "CUDA_VISIBLE_DEVICES": "1"
            }
        },
        {
            "name": "Visualize",
            "type": "debugpy",
            "request": "launch",
            "program": "visualize.py",
            "console": "integratedTerminal"
        }
    ]
}